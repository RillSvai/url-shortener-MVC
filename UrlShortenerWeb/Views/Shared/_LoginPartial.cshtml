@inject IUserManager<User> _userManager;

@if (!_userManager.IsSignedIn(SD.User!))
{
    <ul class="navbar-nav end-100">

        <li class="nav-item">
            <a class="nav-link" asp-area="" asp-controller="Identity" asp-action="Register">Register</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-area="" asp-controller="Identity" asp-action="Login">Login</a>
        </li>
    </ul>
}
else
{
    <ul class="end-100 navbar-nav">
        <li class="nav-item">
            <span class="nav-link">Hello @SD.User?.UserName!</span>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-area="" asp-controller="Identity" asp-action="Logout">Logout</a>
        </li>
    </ul>
}